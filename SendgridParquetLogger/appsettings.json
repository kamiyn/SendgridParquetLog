{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "S3": {
    "AccessKey": "${S3_ACCESS_KEY}",
    "SecretKey": "${S3_SECRET_KEY}",
    "ServiceUrl": "${S3_SERVICE_URL}",
    "BucketName": "${S3_BUCKET_NAME}"
  }
}
